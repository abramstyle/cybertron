    "@babel/runtime": "^7.2.0",
    "autoprefixer": "^9.4.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-css-modules": "^5.0.0",
    "bluebird": "^3.5.3",
    "cli-color": "^1.4.0",
