{
  "name": "kaonjs",
  "version": "2.1.3",
  "description": "Kaon.js is a react isomorphic framework, render your code from server side, with automatic code splitting",
  "main": "index.js",
  "repository": "https://github.com/abramstyle/kaonjs",
  "author": "Abramstyle <abram.style@gmail.com>",
  "bin": {
    "kaon": ".bin/cli.js"
  },
  "scripts": {
    "start": "NODE_ENV=development .bin/cli.js start --config ./example/config/cybertron.config.js",
    "dev": "NODE_ENV=development .bin/cli.js start --config ./example/config/cybertron.config.js",
    "prod": "NODE_ENV=production .bin/cli.js start --config ./example/config/cybertron.config.js",
    "server": "NODE_ENV=development nodemon server",
    "build:server": "webpack --config config/webpack/server.config.js --watch --env=development --progress --colors",
    "lint": "eslint ./",
    "coverage": "yarn run test && yarn run append",
    "test": "jest --coverage",
    "append": "coveralls < coverage/lcov.info"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@loadable/babel-plugin": "^5.0.1",
    "@loadable/server": "^5.2.0",
    "@loadable/webpack-plugin": "^5.2.1",
    "autoprefixer": "^9.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-css-modules": "^5.0.0",
    "bluebird": "^3.5.1",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.11",
    "cli-color": "^1.2.0",
    "clui": "^0.3.6",
    "css-modules-require-hook": "^4.2.3",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fanas": "^0.8.16",
    "fs-extra": "^5.0.0",
    "file-loader": "^1.1.11",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-favicon": "^2.0.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.4",
    "lodash": "^4.17.5",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss": "^7.0.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "precss": "^3.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "serialize-javascript": "^1.5.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.26.1",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.2",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^23.6.0",
    "pre-commit": "^1.2.2"
  }
}
